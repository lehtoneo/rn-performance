{
  "build": {
    "base": {
      "android": {
        "image": "latest",
        "env": {
          "PLATFORM": "android"
        },
        "cache": {
          "disabled": true
        }
      },
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest",
        "env": {
          "PLATFORM": "ios"
        },
        "cache": {
          "disabled": true
        }
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "ios": {
        "scheme": "dev-rn-performance"
      },
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "test": {
      "ios": {
        "scheme": "rn-performance"
      },
      "android": {
        "buildType": "apk"
      },
      "extends": "base",
      "channel": "test"
    }
  },
  "submit": {
    "test": {}
  },
  "cli": {
    "version": ">= 3.9.2",
    "requireCommit": true
  }
}
